{
  "name": "@universal-deposits/sdk",
  "private": true,
  "version": "1.0.0",
  "description": "Universal Deposits SDK",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "license": "MIT",
  "scripts": {
    "compile": "tsc",
    "prebuild": "node scripts/copy-abis.js",
    "build": "tsc && esbuild src/index.ts --bundle --minify --sourcemap --outfile=dist/index.mjs --format=esm && esbuild src/index.ts --bundle --minify --sourcemap --outfile=dist/index.cjs --format=cjs",
    "prettier:check": "npx prettier -c \"**/*.{js,ts,md,sol,json,yml,yaml}\"",
    "prettier:fix": "npx prettier -w \"**/*.{js,ts,md,sol,json,yml,yaml}\"",
    "test:getName": "tsx index.ts",
    "test:quote": "tsx test-quotes.js",
    "example": "tsx examples/basic-usage.ts"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "esbuild": "^0.24.2",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.21.0",
    "viem": "^2.22.15"
  },
  "dependencies": {
    "@lifi/sdk": "^3.7.7",
    "axios": "^1.9.0"
  },
  "peerDependencies": {
    "viem": "^2.0.0"
  }
}
